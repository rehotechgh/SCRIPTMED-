<body class="sub_page">

    <div class="hero_area">
        <!-- header section strats -->
        <header class="header_section">
            <div class="container">
                <nav class="navbar navbar-expand-lg custom_nav-container ">
                    <a class="navbar-brand" href="index.html">
                        <span>
                            scriptMEDD

                        </span>
                    </a>

                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class=""> </span>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link" href="/">Home </a>
                            </li>
                        
                          


                        </ul>
                    </div>
                </nav>
            </div>
        </header>
        <!-- end header section -->
    </div>

    <!-- about section -->
    <section>
        <div class="container  ">
            <div class="row">
                <div class="col-md-12 ">
                    <div class="container">
                        <br />
                        <div class="row">
                            <h4>OPD | Report <hr /></h4>
                        </div>
                    </div>
                    <div class="control-section">
                        @Html.EJS().Grid("LocalData").DataSource((IEnumerable<object>)ViewBag.OPD).Columns(col =>
                                 {
                                     col.Field("Id").HeaderText("ID").Width("120").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Right).IsPrimaryKey(true).ValidationRules(new { required = true, number = true }).Add();
                                     col.Field("RegistrationNumber").HeaderText("AGMHNO.").Width("150").ValidationRules(new { required = true, minLength = 5 }).Add();
                                     col.Field("Surname").HeaderText("Surname").Width("130").Format("yMd").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Right).EditType("datepickeredit").Add();
                                     col.Field("OtherNames").HeaderText("Other Names").Width("150").Format("C2").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Right).EditType("numericedit").ValidationRules(new { required = true, min = 0 }).Add();
                                     col.Field("Weight").HeaderText("Weight").Width("150").Format("yMd").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Right).EditType("datepickeredit").Add();
                                     col.Field("BloodGroup").HeaderText("Blood Group").Width("150").EditType("dropdownedit").Add();
                                     col.Field("BloodPressure").HeaderText("Blood Pressure").Width("150").EditType("dropdownedit").Add();
                                     col.Field("Temperature").HeaderText("Temperature").Width("150").EditType("dropdownedit").Add();
                                     col.Field("Pulse").HeaderText("Pulse").Width("150").EditType("dropdownedit").Add();
                                     col.Field("Height").HeaderText("Height").Width("150").EditType("dropdownedit").Add();
                                     col.Field("Condition").HeaderText("Condition").Width("150").EditType("dropdownedit").Add();
                                     col.Field("CCC").HeaderText("CCC").Width("150").EditType("dropdownedit").Add();
                                     col.Field("DateCreated").HeaderText("Date").Width("150").EditType("dropdownedit").Add();
                                     

                                    
                                 }).AllowPaging().PageSettings(page => page.PageCount(5)).AllowSorting().AllowFiltering().FilterSettings(filter => { filter.Type(Syncfusion.EJ2.Grids.FilterType.Excel); }).EditSettings(edit => { edit.AllowAdding(true).AllowEditing(true).AllowDeleting(true); }).Toolbar(new List<string>() { "Add", "Edit", "Delete", "Update", "Cancel" }).Render()
                    </div>
                </div>
            </div>
    </section>








    <!-- end about section -->
    <!-- footer section -->
