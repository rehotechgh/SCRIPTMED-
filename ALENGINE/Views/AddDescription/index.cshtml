<body class="sub_page">

    <div class="hero_area">
        <!-- header section strats -->
        <header class="header_section">
            <div class="container">
                <nav class="navbar navbar-expand-lg custom_nav-container ">
                    <a class="navbar-brand" href="index.html">
                        <span>
                            scriptMEDD

                        </span>
                    </a>

                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class=""> </span>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link" href="/">Home </a>
                            </li>
                          
                           
                          


                        </ul>
                    </div>
                </nav>
            </div>
        </header>
        <!-- end header section -->
    </div>

    <!-- about section -->
    <section>
        <div class="container  ">
            <div class="row">
                <div class="col-md-12 ">
                    <div class="container">
                        <br />
                        <div class="row">
                            <h4>Add Drug Prescription <hr /></h4>
                        </div>
                    </div>
                    <div class="control-section">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card-box">

                            <h4 class="card-title">Add Description</h4>
                            <form asp-controller="AddDescription" asp-action="Index" method="post">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                         
                                            <div class="col-md-9">
                                                <input type="text" asp-for="Code" class="form-control" placeholder="Enter Description here">
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                          
                                            <div class="col-md-9">
                                                <input type="text" asp-for="Code" class="form-control" value="A0101" hidden />
                                            </div>
                                        </div>
                                       
                                       
                                    </div>
                              
                                </div>

                         
                          
                                <div class="text-left mb-8">
                                 <button type="submit" class="btn btn-success">Save Description</button>

                                </div>
                                <div>
                                    <script type="text/javascript">
                                        alert("@ViewBag.Message");
                                    </script>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>

            </div>
                    </div>
                </div>
            </div>
    </section>








    <!-- end about section -->
    <!-- footer section -->
    <footer class="footer_section">
        <div class="container">
          
        </div>

    </footer>
    <!-- footer section -->
