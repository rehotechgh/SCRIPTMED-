
@model ALENGINE.Models.ViewModel.ViewANCDetailsModel
<!DOCTYPE html>
<html>

<head>
    <!-- Basic -->
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <!-- Site Metas -->
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <link rel="shortcut icon" href="~/images/favicon.png" type="">

    <title> scriptMEDD </title>

    <!-- bootstrap core css -->
    <link rel="stylesheet" type="text/css" href="~/css/bootstrap.css" />



    <!-- fonts style -->
    <link href="~/css2/custom.css" rel="stylesheet">
    <link href="~/css2/boostraptable.css" rel="stylesheet">
    <link href="~/css2/bootstrap4.css" rel="stylesheet">
    <link href="~/css2/responsive.bootstrap4.css" rel="stylesheet">

    <!--owl slider stylesheet -->
    <link rel="stylesheet" type="text/css" href="~/css2/custom2.css" />

    <!-- font awesome style -->
    <link href="~/css2/font-awesome.min.css" rel="stylesheet" />

    <!-- Custom styles for this template -->
    <link href="~/css2/style.css" rel="stylesheet" />
    <!-- responsive style -->
    <link href="~/css2/responsive.css" rel="stylesheet" />

</head>

<body class="sub_page">

    <div class="hero_area">
        <!-- header section strats -->
        <header class="header_section">
            <div class="container">
                <nav class="navbar navbar-expand-lg custom_nav-container ">
                    <a class="navbar-brand" href="index.html">
                        <span>
                            scriptMEDD

                        </span>
                    </a>

                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class=""> </span>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link" href="/">Home </a>
                            </li>
                          


                        </ul>
                    </div>
                </nav>
            </div>
        </header>
        <!-- end header section -->
    </div>

    <!-- about section -->
    <section>
        <div class="container  ">
            <div class="row">
                <div class="col-md-12 ">
                    <div class="container">
                        <br />
                        <div class="row">
                            <h4>ANC  | Add Delivery Note <hr /></h4>
                        </div>
                    </div>
                    <div class="content">


                        <div class="row">
                            <div class="col-md-12">






                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="">

                                            <h4 class="card-title">Delivery Note Information</h4>

                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="form-group row">
                                                        <label class="col-md-3 col-form-label">Registration Number</label>
                                                        <div class="col-md-9">
                                                            @Html.TextBoxFor(m => m.RegistrationNumber, new { @class = "form-control", @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <label class="col-md-3 col-form-label">Surname</label>
                                                        <div class="col-md-9">
                                                            @Html.TextBoxFor(m => m.Surname, new { @class = "form-control", @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <label class="col-md-3 col-form-label">Othernames</label>
                                                        <div class="col-md-9">
                                                            @Html.TextBoxFor(m => m.OtherNames, new { @class = "form-control", @readonly = "readonly" })
                                                        </div>
                                                    </div>

                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group row">
                                                        <label class="col-md-3 col-form-label">Weight</label>
                                                        <div class="col-md-9">
                                                            @Html.TextBoxFor(m => m.Weight, new { @class = "form-control", @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <label class="col-md-3 col-form-label">Height</label>
                                                        <div class="col-md-9">
                                                            @Html.TextBoxFor(m => m.Height, new { @class = "form-control", @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <label class="col-md-3 col-form-label">Blood Pressure</label>
                                                        <div class="col-md-9">
                                                            @Html.TextBoxFor(m => m.BloodPressure, new { @class = "form-control", @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <label class="col-md-3 col-form-label">Temperature</label>
                                                        <div class="col-md-9">
                                                            @Html.TextBoxFor(m => m.Temperature, new { @class = "form-control", @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <label class="col-md-3 col-form-label">CCC</label>
                                                        <div class="col-md-9">
                                                            @Html.TextBoxFor(m => m.CCC, new { @class = "form-control", @readonly = "readonly" })
                                                        </div>
                                                    </div>

                                                    <div class="form-group row">
                                                        <label class="col-md-3 col-form-label">Condition</label>
                                                        <div class="col-md-9">
                                                            @Html.TextBoxFor(m => m.Condition, new { @class = "form-control", @readonly = "readonly" })
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>




                                        </div>
                                    </div>
                                </div>





                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div>

                                <h4 class="card-title">Daily Vitals Information</h4>



                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="col-md-3 col-form-label">Weight</label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" asp-for="Weight" required />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-md-3 col-form-label">Blood Pressure</label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" asp-for="BloodPressure" required />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-md-3 col-form-label">Temperature</label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" asp-for="Temperature" required />
                                            </div>
                                        </div>

                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group row">
                                            <label class="col-md-3 col-form-label">Pulse</label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" asp-for="Pulse" required />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-md-3 col-form-label">SPO2</label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" asp-for="SP02" required />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-md-3 col-form-label"></label>
                                            <div class="col-md-9">
                                            </div>
                                        </div>

                                        <div class="form-group row">
                                            <label class="col-md-3 col-form-label"></label>
                                            <div class="col-md-9">
                                            </div>
                                        </div>

                                    </div>
                                </div>




                            </div>
                        </div>
                    </div>
                  
                    <br />
                    <br />
                    <div class="content">


                        <div class="row">
                            <div class="col-md-12">
                                <div>

                                    <h4 class="card-title">Daily Observation/Findings</h4>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <label class="col-md-3 col-form-label">Observation</label>
                                            <div class="col-md-9">
                                                <textarea class="form-control" asp-for="Observation" required></textarea>
                                            </div>

                                        </div>
                                        <div class="col-md-6">
                                            <label class="col-md-3 col-form-label">Delivery Note</label>
                                            <div class="col-md-9">
                                                <textarea class="form-control" asp-for="Deliverynote" required></textarea>
                                            </div>

                                        </div>
                                    </div>




                                </div>
                            </div>
                        </div>
                        <br/>
                        <br/>




                    </div>
                    <div class="content">
                        <div class="row">
                            <div class="col-md-12">
                                <div>

                                    <h4 class="card-title">Delivered Baby Information if Due</h4>



                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group row">
                                                <label class="col-md-3 col-form-label">Gestational Age</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" asp-for="GestationalAge" required />
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <label class="col-md-3 col-form-label">Time of Delivery</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" asp-for="DeliveryTime" required />
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <label class="col-md-3 col-form-label">Weight of Baby</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" asp-for="WeightOfBaby" required />
                                                </div>
                                            </div>

                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group row">
                                                <label class="col-md-3 col-form-label">Apgar Score of the Baby</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" asp-for="ApgarScoreOfBaby" required />
                                                </div>
                                            </div>
                                            <div class="form-group row">

                                                <div class="col-md-9">
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <label class="col-md-3 col-form-label"></label>
                                                <div class="col-md-9">
                                                </div>
                                            </div>

                                            <div class="form-group row">
                                                <label class="col-md-3 col-form-label"></label>
                                                <div class="col-md-9">
                                                </div>
                                            </div>

                                        </div>
                                    </div>




                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <div class="card-box">



                                    <div class="row">
                                        <div class="col-md-6">
                                        </div>
                                        <div class="col-md-6">

                                            <div class="row">

                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <input type="reset" value="RESET" class="form-control" />
                                                    </div>
                                                    <div class="col-md-6">
                                                        <input type="submit" value="SAVE" class="form-control" />
                                                    </div>

                                                </div>

                                            </div>
                                            <div>
                                                <script type="text/javascript">
                                                    alert("@ViewBag.Message");</script>
                                            </div>
                                        </div>
                                    </div>




                                </div>
                            </div>
                        </div>

                    </div>
                    <br />
                    <br />
                   

                </div>
            </div>
    </section>








    <!-- end about section -->
    <!-- footer section -->
    <footer class="footer_section">
        <div class="container">
            
        </div>

    </footer>
    <!-- footer section -->
    <!-- jQery -->

    <script src="assets/js/jquery-3.2.1.min.js"></script>
    <script src="assets/js/popper.min.js"></script>
    <script src="assets/js/bootstrap.min.js"></script>
    <script src="assets/js/jquery.dataTables.min.js"></script>
    <script src="assets/js/dataTables.bootstrap4.min.js"></script>
    <script src="assets/js/select2.min.js"></script>
    <script src="assets/js/moment.min.js"></script>
    <script src="assets/js/bootstrap-datetimepicker.min.js"></script>
    <script src="assets/js/jquery.slimscroll.js"></script>
    <script src="assets/js/app.js"></script>
    <script type="text/javascript" src="~/js/jquery-3.4.1.min.js"></script>
    <script type="text/javascript" src="~/js2/query-3.7.1.js"></script>
    <script type="text/javascript" src="~/js2/bootstrap.bundle.min.js"></script>
    <script type="text/javascript" src="~/js2/dataTables.js"></script>
    <script type="text/javascript" src="~/js2/data"></script>
    <!-- popper js -->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous">
    </script>
    <!-- bootstrap js -->
    <script type="text/javascript" src="js/bootstrap.js"></script>
    <!-- owl slider -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js">
    </script>
    <!-- custom js -->
    <script type="text/javascript" src="js/custom.js"></script>
    <!-- Google Map -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCh39n5U-4IoWpsVGUHWdqB6puEkhRLdmI&callback=myMap">
    </script>


</body>

</html>