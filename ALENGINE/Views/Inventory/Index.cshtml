                                        </div>
                                        <div class="form-group row">
                                            <label class="col-md-3 col-form-label">Expected Sales</label>
                                            <div class="col-md-9">
                                                <input type="text" asp-for="ExpectedSales" id="Sales" class="form-control" readonly>
                                            </div>
                                        </div>
                                    </div>
                                </div>

