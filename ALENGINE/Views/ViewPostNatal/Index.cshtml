    <!-- about section -->
    <section>
        <div class="container  ">
            <div class="row">
                <div class="col-md-12 ">
                    <div class="container">
                        <br />
                        <div class="row">
                            <h4>ANC  | Post Natal <hr /></h4>
                        </div>
                    </div>
                    <div class="content">


                        <div class="row">
                            <div class="col-md-12">

                                <form method="post" asp-controller="ViewANC" asp-action="index">
                                    <div class="page-wrapper">
                                        <div class="content">


                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div>

                                                        <h4 class="card-title">Baby Information | Post Natal </h4>


                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div>

                                                                    <h4>Vitals Information</h4>



                                                                    <div class="row">
                                                                        <div class="col-md-6">
                                                                            <div class="form-group row">
                                                                                <label class="col-md-3 col-form-label">Registration Number</label>
                                                                                <div class="col-md-9">
                                                                                    <input type="text" class="form-control" asp-for="RegistrationNumber" required />
                                                                                </div>
                                                                            </div>
                                                                            <div class="form-group row">
                                                                                <label class="col-md-3 col-form-label">Surname</label>
                                                                                <div class="col-md-9">
                                                                                    <input type="text" class="form-control" asp-for="Surname" required />
                                                                                </div>
                                                                            </div>
                                                                            <div class="form-group row">
                                                                                <label class="col-md-3 col-form-label">Othernames</label>
                                                                                <div class="col-md-9">
                                                                                    <input type="text" class="form-control" asp-for="OtherNames" required />
                                                                                </div>
                                                                            </div>

                                                                        </div>
                                                                        <div class="col-md-6">
                                                                            <div class="form-group row">
                                                                                <label class="col-md-3 col-form-label">Weight</label>
                                                                                <div class="col-md-9">
                                                                                    <input type="text" class="form-control" asp-for="Weight" required />
                                                                                </div>
                                                                            </div>
                                                                            <div class="form-group row">
                                                                                <label class="col-md-3 col-form-label">Height</label>
                                                                                <div class="col-md-9">
                                                                                    <input type="text" class="form-control" asp-for="Height" required />
                                                                                </div>
                                                                            </div>
                                                                            <div class="form-group row">
                                                                                <label class="col-md-3 col-form-label">Blood Group</label>
                                                                                <div class="col-md-9">
                                                                                    <input type="text" class="form-control" asp-for="BloodGroup" required />
                                                                                </div>
                                                                            </div>
                                                                            <div class="form-group row">
                                                                                <label class="col-md-3 col-form-label">Blood Pressure</label>
                                                                                <div class="col-md-9">
                                                                                    <input type="text" class="form-control" asp-for="BloodPressure" required />
                                                                                </div>
                                                                            </div>
                                                                            <div class="form-group row">
                                                                                <label class="col-md-3 col-form-label">Temperature</label>
                                                                                <div class="col-md-9">
                                                                                    <input type="text" class="form-control" asp-for="Temperature" required />
                                                                                </div>
                                                                            </div>

                                                                            <div class="form-group row">
                                                                                <label class="col-md-3 col-form-label"></label>
                                                                                <div class="col-md-9">
                                                                                </div>
                                                                            </div>

                                                                        </div>
                                                                    </div>




                                                                </div>
                                                            </div>
                                                        </div>




                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="content">
                                            <div class="card-box">

                                                <div class="row">
                                                    <label>Post Natal Note</label>
                                                    <div class="row">
                                                        <div class="col-md-12">

                                                            <div class="form-group">
                                                                <textarea asp-for="Note" class="form-control" required>

                                           </textarea>
                                                            </div>

                                                        </div>


                                                    </div>


                                                </div>



                                            </div>


                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="card-box">

                                                        <div class="row">
                                                            <div class="col-sm-6">
                                                                <label>Type of Services</label>
                                                                <select name="TypeOfService" class="form-control" required>
                                                                    <option selected="selected">Select only one</option>
                                                                    <option value="Outpatients">Outpatients</option>
                                                                    <option value="In-Patients">In-Patients</option>
                                                                    <option value="Diagnostic">Diagnostic</option>
                                                                    <option value="Pharmacy">Pharmacy</option>
                                                                    <option value="All Inclusive">All Inclusive</option>
                                                                    <option value="Unbounded">Unbounded</option>
                                                                </select>
                                                                <div>
                                                                    <label>Outcome</label>
                                                                    <select name="Outcome" class="form-control" required>
                                                                        <option selected="selected">Select Outcome</option>
                                                                        <option value="Discharged">Discharged</option>
                                                                        <option value="Died">Died</option>
                                                                        <option value="Transfered Out">Transfered Out</option>
                                                                        <option value="Absconded/Discharged Against Medical Advice">Absconded/Discharged Against Medical Advice </option>

                                                                    </select>
                                                                </div>

                                                            </div>
                                                            <div class="col-sm-6">
                                                                <label>Visitation Type/Date of Service Provision</label>
                                                                <div class="row">
                                                                    <div class="col-md-6">

                                                                        <input type="text" class="form-control" asp-for="VisitationType" required />
                                                                    </div>
                                                                    <div class="col-md-6">
                                                                        <input type="date" asp-for="VisitationDate" class="form-control" />
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>



                                                    </div>
                                                    <div class="card-box">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="row">
                                                                    <div class="col-md-4">
                                                                        <label>Diagnos(es)</label>
                                                                    </div>
                                                                    <div class="col-md-4">
                                                                        <label>Drug(es)</label>
                                                                    </div>
                                                                    <div class="col-md-4">
                                                                        <label>Dossag(es)</label>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row">

                                                            <div class="col-sm-12">


                                                                <div class="row">
                                                                    <div class="col-md-4">

                                                                        <div class="form-group">

                                                                            <select asp-for="Diagnosis1" asp-items="@ViewBag.Description" class="form-control">
                                                                                <option value="-1" selected="selected">---Select Description---</option>
                                                                            </select>
                                                                        </div>
                                                                        <div class="form-group">

                                                                            <select asp-for="Diagnosis2" asp-items="@ViewBag.Description" class="form-control">
                                                                                <option value="-1" selected="selected">---Select Description---</option>
                                                                            </select>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <select asp-for="Diagnosis3" asp-items="@ViewBag.Description" class="form-control">
                                                                                <option value="-1" selected="selected">---Select Description---</option>
                                                                            </select>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <select asp-for="Diagnosis4" asp-items="@ViewBag.Description" class="form-control">
                                                                                <option value="-1" selected="selected">---Select Description---</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>


                                                                    <div class="col-md-4">

                                                                        <div class="form-group">
                                                                            <select asp-for="DICD1" asp-items="@ViewBag.Medlist" class="form-control">
                                                                                <option value="-1" selected="selected">---Select Medicine---</option>
                                                                            </select>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <select asp-for="DICD2" asp-items="@ViewBag.Medlist" class="form-control">
                                                                                <option value="-1" selected="selected">---Select Medicine---</option>
                                                                            </select>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <select asp-for="DICD3" asp-items="@ViewBag.Medlist" class="form-control">
                                                                                <option value="-1" selected="selected">---Select Medicine---</option>
                                                                            </select>
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <select asp-for="DICD4" asp-items="@ViewBag.Medlist" class="form-control">
                                                                                <option value="-1" selected="selected">---Select Medicine---</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-4">
                                                                        <div class="form-group">
                                                                            <input type="text" asp-for="DIDRG1" class="form-control" placeholder="Dossage" required />
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <input type="text" asp-for="DIDRG2" class="form-control" placeholder="Dossage" required />
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <input type="text" asp-for="DIDRG3" class="form-control" placeholder="Dossage" required />
                                                                        </div>
                                                                        <div class="form-group">
                                                                            <input type="text" asp-for="DIDRG4" class="form-control" placeholder="Dossage" required />

                                                                        </div>

                                                                    </div>


                                                                </div>

                                                            </div>

                                                        </div>



                                                    </div>
                                                </div>
                                            </div>
                                            <!--INVESTIGATIONS-->

                                            <div class="content">


                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="card-box">

                                                            <h4 class="card-title">Lab Request/Dispensary/Appointment</h4>

                                                            <div class="row">
                                                                <div class="col-md-6">
                                                                    <div class="form-group row">
                                                                        <label class="col-md-3 col-form-label">Lab Request</label>
                                                                        <div class="col-md-9">
                                                                            <select class="form-control" asp-for="LabRequest" required>
                                                                                <option select>Select</option>
                                                                                <option>Yes</option>
                                                                                <option>No</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>
                                                                    <div class="form-group row">

                                                                        <label class="col-md-3 col-form-label">Lab Type</label>
                                                                        <div class="col-md-9">
                                                                            <input type="text" class="form-control" asp-for="LabType" required />
                                                                        </div>
                                                                    </div>
                                                                    <div class="form-group row">
                                                                        <label class="col-md-3 col-form-label">Dispensary</label>
                                                                        <div class="col-md-9">
                                                                            <select class="form-control" asp-for="Dispensary" required>
                                                                                <option select>Select</option>
                                                                                <option>Yes</option>
                                                                                <option>No</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>
                                                                    <div class="form-group row">

                                                                        <label class="col-md-3 col-form-label">Dispensary Type</label>
                                                                        <div class="col-md-9">
                                                                            <select class="form-control" asp-for="DispensaryType" required>
                                                                                <option selected>Select</option>
                                                                                <option>Own Facility</option>
                                                                                <option>Off-Premise</option>

                                                                            </select>
                                                                        </div>
                                                                    </div>

                                                                    <div class="form-group row">
                                                                    </div>

                                                                </div>
                                                                <div class="col-md-6">
                                                                    <div class="form-group row">
                                                                        <label class="col-md-3 col-form-label">Set Appointment</label>
                                                                        <div class="col-md-9">
                                                                            <select class="form-control" asp-for="SetAppointment">
                                                                                <option selected>Select</option>
                                                                                <option>Yes</option>
                                                                                <option>No</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>
                                                                    <div class="form-group row">
                                                                    </div>
                                                                    <div class="form-group row">
                                                                    </div>

                                                                    <div class="form-group row">
                                                                        <label class="col-md-3 col-form-label">Appointment Date</label>
                                                                        <div class="col-md-9">
                                                                            <input type="date" class="form-control" asp-for="AppointmentDate" />
                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>




                                                        </div>
                                                    </div>
                                                </div>

                                            </div>

                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="card-box">



                                                        <div class="row">
                                                            <div class="col-md-6">
                                                            </div>
                                                            <div class="col-md-6">

                                                                <div class="row">

                                                                    <div class="row">
                                                                        <div class="col-md-6">
                                                                            <input type="reset" value="RESET" class="form-control" />
                                                                        </div>
                                                                        <div class="col-md-6">
                                                                            <input type="submit" value="SAVE" class="form-control" />
                                                                        </div>

                                                                    </div>

                                                                </div>
                                                                <br/>
                                                                <br/>
                                                                <div>
                                                                    <script type="text/javascript">
                                                                        alert("@ViewBag.Message");</script>
                                                                </div>
                                                            </div>
                                                        </div>




                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                   
                                    </div>
                                </form>




                              





                            </div>
                        </div>

                    </div>
                   
    </section>








